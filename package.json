{
  "name": "cosmosdb-movie-ai",
  "version": "1.0.0",
  "type": "module",
  "description": "Hello World TypeScript CLI for querying Cosmos DB movie data with AI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "load-data": "node dist/load-data.js",
    "vectorize": "node dist/vectorize.js",
    "demo": "node dist/demo.js",
    "test": "vitest run --exclude='**/integration/**'",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "vitest run tests/e2e/",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --exclude='**/integration/**'",
    "test-config": "node dist/test-config.js",
    "verify": "./verify-setup.sh",
    "clean": "rm -rf dist coverage",
    "setup": "npm install && npm run build"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "dotenv": "^16.5.0",
    "openai": "^4.20.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "typescript": "^5.0.0",
    "vitest": "^3.2.3"
  }
}
